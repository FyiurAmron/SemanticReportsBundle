{% block notice_area %}
	{% if error is defined %}
		<div class='ui warning message'>
			{{error}}
		</div>
	{% endif %}

	{% if notice is defined %}
		<div class='ui info message'>
			{{notice}}
		</div>
	{% endif %}
{% endblock %}

{% block content %}
	{% if error is not defined %}
		{% include "@SemanticReports/Default/html/report_content.twig" %}
	{% endif %}
{% endblock %}
